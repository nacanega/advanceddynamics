\NewDocumentCommand{\entry}{mmmmmmm}{% Output
    \vspace{-0.5em}%
    \subsection{\comsec{#1}}\nopagebreak%
    \label{entry:#1}%
    \begin{docCommand}[blank,halo=0in with black!10!white]{#1}{#2}%
        \vspace{0.25em}%
        #3%
        \vspace{0.5em}\\%
        {\setlength\tabcolsep{2pt} #4}%
    \end{docCommand}%
    \begin{tcolorbox}[title=Example,%
        boxrule=2pt,%
        colback=black!0!white,%
        colframe=black!75!white,%
        coltitle=black!0!white,%
        fonttitle=\bfseries,%
        subtitle style={%
            boxrule=0pt,%
            colframe=black!75!white,%
            colback=black!15!white,%
            colupper=black!100!white}%
        ]%
        #5%
        \tcblower%
        This is accomplished with the \LaTeX\ below:%
        \begin{tcolorbox}[blank,halo=0in with black!10!white]%
            #6%
        \end{tcolorbox}%
        \tcbsubtitle{Display/Inline Mode Output}%
        \vspace{-1.5em}%
        \begin{align*}%
            #7 %
        \end{align*}%
        \vspace{-2em}%
    \end{tcolorbox}%
}

